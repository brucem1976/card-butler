{{!--need to loop this puppy into existence--}}
<link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css"/>

<div class="container-fluid">
  {{#if i}}
  <div class="row">
    {{getCardImage i 0}}
    {{getCardImage i 1}}
  </div>
  {{/if}}

  <div class="row">
    {{getCardImage i 20}}
    {{getCardImage i 21}}
    {{getCardImage i 22}}
    {{getCardImage i 23}}
    {{getCardImage i 24}}
  </div>

  {{#if i}}
  <div class="row">
    <h2 style="color:red">{{getRank i}}</h2>
    <h3>Ranking:</h3>
    <p id="10">Royal Flush</p>
    <p id="9">Straight Flush</p>
    <p id="8">Four of a Kind</p>
    <p id="7">Full House</p>
    <p id="6">Flush</p>
    <p id="5">Straight</p>
    <p id="4">Three of a Kind</p>
    <p id="3">Two Pair</p>
    <p id="2">Pair</p>
    <p id="1">High Card</p>
  </div>
  {{/if}}
  
  {{#unless i}}
  {{!--<button>Next!</button>--}}
  {{/unless}}
</div>